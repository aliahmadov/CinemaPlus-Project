	<header>
		<nav class="myNav">
			<section class="myNav-left">
				<img class="logoImg" alt="logo" src="~/assets/images/cpluslogo.svg">
			</section>

			<section class="myNav-right">
				<section class="myNav-right-up">
					<div class="link-container ml-3-1">
						<a href="/Home/Platinum">Platinum</a>
					</div>
					<div class="link-container">
						<a href="/Home/DolbyAtmos">Dolby Atmos</a>
					</div>
					<div class="link-container">
						<a href="/Home/Services">Services</a>
					</div>
					<div class="link-container">
						<a href="/Home/About">About us</a>
					</div>
				</section>

				<section class="myNav-right-down">
					<div class="menu-con ml-3">
						<a href="/Home">Home</a>
					</div>
					<div class="menu-con">
						<a href="/Home/Campaigns">Campaigns</a>
					</div>
					<div class="menu-con">
						<a href="/Home/Tariffs">Tariffs</a>
					</div>
					<div class="menu-con">
						<a href="/Home/CineBonus">CineBonus</a>
					</div>

					<div class="my-contact">
						<i style="color: white; margin-right: 5px;" class="fa-solid fa-phone"></i>
						+99412 499 89 88
					</div>
				</section>

				<section class="hamburger-menu">
					<div style="width: 100%;padding: 10px;">
						<i style="font-size: 50px;" class="fa-solid fa-bars" id="hamburgerBtn"></i>

						<div class="hamburger-dropdown" id="hamburgerDropdown">
							<div class="hamDiv" style="margin-top: 20px;">
								<a href="/Home/Platinum">Platinum</a>
							</div>

							<div class="hamDiv">
								<a href="/Home/Campaigns">Campaigns</a>
							</div>

							<div class="hamDiv">
								<a href="/Home/Tariffs">Tariffs</a>
							</div>

							<div class="hamDiv">
								<a href="/Home/About">About us</a>
							</div>

							<div class="hamDiv">
								<a href="/Home/Contacts">Contacts</a>
							</div>
						</div>
					</div>
				</section>
			</section>
